  pages = {470-485; discussion 485-489},
  keywords = {Humans,Physical Therapy Modalities,Acute Disease,Algorithms,Chronic Disease,Low Back Pain,Pain Measurement,Physical Examination,Questionnaires,Sick Role,Syndrome},
  author = {Delitto, A and Erhard, R E and Bowling, R W},
  file = {C\:\\Users\\PERSONAL\\Zotero\\storage\\IEUXCCI4\\delitto.PDF},
  eprinttype = {pmid},
