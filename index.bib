  journaltitle = {PLoS ONE},
  shortjournal = {PLoS One},
  urldate = {2018-10-04},
  date = {2013-02-06},
  author = {Falagas, Matthew E. and Zarkali, Angeliki and Karageorgopoulos, Drosos E. and Bardakas, Vangelis and Mavros, Michael N.},
