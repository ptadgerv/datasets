@article{blankenburgPreoperativeCYFRA2112008,
  langid = {english},
  title = {Preoperative {{CYFRA}} 21-1 and {{CEA}} as {{Prognostic Factors}} in {{Patients}} with {{Stage I Non}}-{{Small Cell Lung Cancer}}},
  volume = {29},
  issn = {1010-4283, 1423-0380},
  url = {https://www.karger.com/Article/FullText/152945},
  doi = {10.1159/000152945},
  abstract = {\emph{Objective:} To validate the prognostic value of preoperative levels of CYFRA 21-1, CEA and the corresponding tumor marker index (TMI) in patients with stage I non-small cell lung cancer (NSCLC). \emph{Methods:} Two hundred forty stage I NSCLC patients (80 in pT1 and 160 in pT2; 100 squamous cell carcinomas, 91 adenocarcinomas, 32 large-cell carcinomas, 17 with other histologies; 171 males and 69 females) who had complete resection (R0) between 1986 and 2004 were included in the analysis. CYFRA 21-1 and CEA were measured using the Elecsys system (Roche) and AxSym-System (Abbott), respectively. Univariate analysis was performed using the Kaplan-Meier method to identify potential associations between survival and age, gender, CYFRA 21-1, CEA and TMI. \emph{Results:} Overall 3- and 5-year survival rates were 74 and 64\%, respectively. Male gender (p = 0.0009) and age {$>$}70 years (p = 0.0041) were associated with a worse prognosis; there were no differences between pT1 and pT2 nor between histological subtypes. Three-year survival was 72\% for CYFRA 21-1 levels {$>$}3.3 ng/ml versus 75\% for levels {$\leq$}3.3 ng/ml, 71\% for CEA {$>$} 6.7 ng/ml versus 75\% for CEA {$\leq$}6.7 ng/ml (both p values {$>$}0.05). Corresponding 5-year survival rates were near 64\% both for patients with CYFRA 21-1 values above and below the cutoff (3.3 ng/ml), and 49 and 66\% for patients with values above and below the CEA cutoff (6.7 ng/ml), respectively (both p values {$>$}0.05). Overall survival did not vary in the different TMI risk groups (p = 0.73). \emph{Conclusions:} In this cohort of early-stage NSCLC patients, male gender and age {$>$}70 years were associated with a worse outcome, but elevated levels of CEA and CYFRA 21-1, and TMI risk were not.},
  number = {4},
  journaltitle = {Tumor Biology},
  shortjournal = {TBI},
  urldate = {2019-10-17},
  date = {2008},
  pages = {272-277},
  author = {Blankenburg, Florian and Hatz, Rudolf and Nagel, Dorothea and Ankerst, Donna and Reinmiedl, Judith and Gruber, Christine and Seidel, Dietrich and Stieber, Petra},
  file = {C\:\\Users\\PERSONAL\\Zotero\\storage\\W4G2VZFH\\Blankenburg et al. - 2008 - Preoperative CYFRA 21-1 and CEA as Prognostic Fact.pdf;C\:\\Users\\PERSONAL\\Zotero\\storage\\S95UZ8C9\\152945.html},
  eprinttype = {pmid},
  eprint = {18781099}
}

