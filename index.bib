@article{christiansenEffectsSingleSession2018,
  title = {The Effects of a Single Session of Spinal Manipulation on Strength and Cortical Drive in Athletes.},
  volume = {118},
  issn = {1439-6327},
  url = {http://www.ncbi.nlm.nih.gov/pubmed/29327170 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC5843672},
  doi = {10.1007/s00421-018-3799-x},
  abstract = {PURPOSE The primary purpose of this study was to investigate whether a single session of spinal manipulation (SM) increases strength and cortical drive in the lower limb (soleus muscle) of elite Taekwondo athletes. METHODS Soleus-evoked V-waves, H-reflex and maximum voluntary contraction (MVC) of the plantar flexors were recorded from 11 elite Taekwondo athletes using a randomized controlled crossover design. Interventions were either SM or passive movement control. Outcomes were assessed at pre-intervention and at three post-intervention time periods (immediate post, post 30 min and post 60 min). A multifactorial repeated measures ANOVA was conducted to assess within and between group differences. Time and session were used as factors. A post hoc analysis was carried out, when an interactive effect was present. Significance was set at p {$\leq$} 0.05. RESULTS SM increased MVC force [F(3,30) = 5.95, p \textbackslash{}textless 0.01], and V-waves [F(3,30) = 4.25, p = 0.01] over time compared to the control intervention. Between group differences were significant for all time periods (p \textbackslash{}textless 0.05) except for the post60 force measurements (p = 0.07). CONCLUSION A single session of SM increased muscle strength and corticospinal excitability to ankle plantar flexor muscles in elite Taekwondo athletes. The increased MVC force lasted for 30 min and the corticospinal excitability increase persisted for at least 60 min.},
  number = {4},
  journaltitle = {European journal of applied physiology},
  date = {2018-04},
  pages = {737--749},
  keywords = {Athletic performance,Fatigue,H-reflex,Maximum voluntary contraction force,MVC,Spinal manipulation,Taekwondo,V-wave},
  author = {Christiansen, Thomas Lykke and Niazi, Imran Khan and Holt, Kelly and Nedergaard, Rasmus Wiberg and Duehr, Jens and Allen, Kathryn and Marshall, Paul and T\"urker, Kemal S and Hartvigsen, Jan and Haavik, Heidi},
  file = {C\:\\Users\\PERSONAL\\Zotero\\storage\\GCP6ZLZJ\\Christiansen et al. - 2018 - The effects of a single session of spinal manipulation on strength and cortical drive in athletes.pdf},
  eprinttype = {pmid},
