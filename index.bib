@article{meskersVivoEstimationGlenohumeral1997,
  title = {In Vivo Estimation of the Glenohumeral Joint Rotation Center from Scapular Bony Landmarks by Linear Regression},
  volume = {31},
  issn = {0021-9290},
  url = {http://www.sciencedirect.com.roble.unizar.es:9090/science/article/B6T82-418XNW9-F/2/6b08f7ace26de454c290e259695e0f30},
  doi = {10.1016/S0021-9290(97)00101-2},
  abstract = {In this paper, a method is described for in vivo prediction of the glenohumeral joint rotation center (GH-r), necessary for the construction of a humerus local coordinate system in shoulder kinematic studies. The three-dimensional positions of five scapula bony landmarks as well as a large number of data points on the surface of the glenoid and humeral head were collected at 36 sets of cadaver scapulae and adjacent humeri. The position of GH-r in each scapula was estimated by mathematically fitting spheres to the glenoid and humeral head. GH-r prediction from scapula geometry parameters by linear regression resulted in a RMSE between measured and predicted GH-r of 2.32 mm for the x-coordinate, 2.69 mm for the y-coordinate and 3.04 mm for the z-coordinate. Application in vivo revealed a random humerus orientation error due to measurement inaccuracies of 1.35, 0.29 and 1.26\textdegree{} standard deviation per rotation angle. The estimated total humerus orientation error including the offset error due to the regression model inaccuracy was 2.86, 0.84 and 2.69\textdegree{} standard deviation. As these errors were about 15 and 20\% of, respectively, the intra- and inter-subject variability of the humerus orientations measured, it is concluded that the method described in this paper allows for an adequate construction of a humerus local coordinate system.},
  number = {1},
  journaltitle = {Journal of Biomechanics},
  urldate = {2010-02-04},
  date = {1997-11-14},
  pages = {93-96},
  keywords = {Shoulder kinematics,Glenohumeral joint,Bony landmarks,Linear regression,Rotation center},
  author = {Meskers, C.G.M. and van der Helm, F.C.T. and Rozendaal, L.A. and Rozing, P.M.},
  options = {useprefix=true},
  file = {C\:\\Users\\PERSONAL\\Zotero\\storage\\E6DH9I64\\6CE31A49d01.pdf;C\:\\Users\\PERSONAL\\Zotero\\storage\\7GGSTRIN\\science.html}
}

