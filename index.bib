  journaltitle = {Journal of Psychosomatic Research},
  shortjournal = {J Psychosom Res},
  date = {2014-12},
  pages = {558-561},
  keywords = {Humans,Adult,Female,Male,Sex Factors,Surveys and Questionnaires,Joint Instability,Anxiety,Anxiety Disorders,Autonomic dysfunction,Brazil,Joint hypermobility,Sex Distribution,Students,Universities,University students,Women,Young Adult},
  author = {Sanches, S. B. and Os\'orio, F. L. and Louzada-Junior, P. and Moraes, D. and Crippa, J. a. S. and Mart\'in-Santos, R.},
